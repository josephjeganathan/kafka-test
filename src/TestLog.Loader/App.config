<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <runtime>
    <!--<gcServer enabled="true"/>
    <gcConcurrent enabled="true"/>-->
  </runtime>
  <appSettings>
    <add key="Kafka.Servers" value="http://josephkafka.cloudapp.net:9092,http://josephkafka.cloudapp.net:9093,http://josephkafka.cloudapp.net:9094,http://josephkafka.cloudapp.net:9095,http://josephkafka.cloudapp.net:9096,http://josephkafka.cloudapp.net:9097" />
    <add key="Kafka.MaximumAsyncQueue" value="64" />
    <add key="Kafka.BatchSize" value="1000" />
  </appSettings>
</configuration>